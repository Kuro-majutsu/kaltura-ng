{
  "compilerOptions": {
    "baseUrl": "src",
    "outDir": "./.tmp",
    "declaration": true,
    "stripInternal": true,
    "experimentalDecorators": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "noEmitHelpers": false,
    "module": "es2015",
    "importHelpers": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "inlineSources": true,
    "target": "es5",
    "skipLibCheck": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@angular/compiler": [
        "../node_modules/@angular/compiler"
      ],
      "@angular/compiler/*": [
        "../node_modules/@angular/compiler/*"
      ],
      "@angular/forms": [
        "../node_modules/@angular/forms"
      ],
      "@angular/forms/*": [
        "../node_modules/@angular/forms/*"
      ],
      "@angular/router": [
        "../node_modules/@angular/router"
      ],
      "@angular/router/*": [
        "../node_modules/@angular/router/*"
      ],
      "@kaltura-ng/kaltura-client": [
        "../node_modules/@kaltura-ng/kaltura-client"
      ],
      "@kaltura-ng/kaltura-client/*": [
        "../node_modules/@kaltura-ng/kaltura-client/*"
      ],
      "@kaltura-ng/kaltura-common": [
        "../node_modules/@kaltura-ng/kaltura-common"
      ],
      "@kaltura-ng/kaltura-common/*": [
        "../node_modules/@kaltura-ng/kaltura-common/*"
      ],
      "@kaltura-ng/kaltura-ui": [
        "../node_modules/@kaltura-ng/kaltura-ui"
      ],
      "@kaltura-ng/kaltura-ui/*": [
        "../node_modules/@kaltura-ng/kaltura-ui/*"
      ],
      "kaltura-typescript-client": [
        "../node_modules/kaltura-typescript-client"
      ],
      "kaltura-typescript-client/*": [
        "../node_modules/kaltura-typescript-client/*"
      ],
      "ng2-translate": [
        "../node_modules/ng2-translate"
      ],
      "ng2-translate/*": [
        "../node_modules/ng2-translate/*"
      ],
      "ramda": [
        "../node_modules/ramda"
      ],
      "ramda/*": [
        "../node_modules/ramda/*"
      ],
      "rxjs": [
        "../node_modules/rxjs"
      ],
      "rxjs/*": [
        "../node_modules/rxjs/*"
      ],
      "tslib": [
        "../node_modules/tslib"
      ],
      "tslib/*": [
        "../node_modules/tslib/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "src/**/*.spec.ts"
  ],
  "angularCompilerOptions": {
    "skipTemplateCodegen": true
  }
}