{
  "compilerOptions": {
    "baseUrl": "src",
    "outDir": "./.tmp",
    "declaration": true,
    "stripInternal": true,
    "experimentalDecorators": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "noEmitHelpers": false,
    "module": "es2015",
    "importHelpers": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "inlineSources": true,
    "target": "es5",
    "skipLibCheck": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@angular/common": [
        "../node_modules/@angular/common"
      ],
      "@angular/common/*": [
        "../node_modules/@angular/common/*"
      ],
      "@angular/core": [
        "../node_modules/@angular/core"
      ],
      "@angular/core/*": [
        "../node_modules/@angular/core/*"
      ],
      "@angular/forms": [
        "../node_modules/@angular/forms"
      ],
      "@angular/forms/*": [
        "../node_modules/@angular/forms/*"
      ],
      "@kaltura-ng/kaltura-common": [
        "../node_modules/@kaltura-ng/kaltura-common"
      ],
      "@kaltura-ng/kaltura-common/*": [
        "../node_modules/@kaltura-ng/kaltura-common/*"
      ],
      "jquery": [
        "../node_modules/jquery"
      ],
      "jquery/*": [
        "../node_modules/jquery/*"
      ],
      "moment": [
        "../node_modules/moment"
      ],
      "moment/*": [
        "../node_modules/moment/*"
      ],
      "ramda": [
        "../node_modules/ramda"
      ],
      "ramda/*": [
        "../node_modules/ramda/*"
      ],
      "rxjs": [
        "../node_modules/rxjs"
      ],
      "rxjs/*": [
        "../node_modules/rxjs/*"
      ],
      "tslib": [
        "../node_modules/tslib"
      ],
      "tslib/*": [
        "../node_modules/tslib/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "src/**/*.spec.ts"
  ],
  "angularCompilerOptions": {
    "skipTemplateCodegen": true
  }
}